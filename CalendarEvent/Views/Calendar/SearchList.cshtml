@model CalendarEvent.Models.EventSchedulerViewModel
<button type="button" id="calendar-btn-back-to-month" class="btn btn-outline-secondary mb-2">
    <i class="fas fa-calendar-alt"></i>
    Calendar
</button>
<table class="table table-sm table-bordered table-hover">
    <tbody>
        @foreach (var item in Model.DataLists)
        {
            <tr style="">
                <td style="width:9rem;">
                    @CalendarEvent.Utility.TimeUtility.ConvertDateToString(item.ef.Eventdate)
                </td>
                <td style="width:15rem;" day="@CalendarEvent.Utility.TimeUtility.ConvertDateToString(item.ef.Eventdate)">
                    <span  class="btn-link btn-edit-note" style="cursor:pointer;" eventid="@item.ef.Eventid" notetitle="@item.ef.Title" notedesc="@item.ef.Description">
                        @item.ef.Title
                    </span>
                </td>
                <td>
                    @item.ef.Description
                </td>
            </tr>
        }
    </tbody>
</table>